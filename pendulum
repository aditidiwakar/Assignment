#include<stdio.h>
int main()
{
int size;
    scanf("%d",&size);
int arr[size];
        for(int i=0;i<size;i++)
        scanf("%d",&arr[i]);
    for(int i=0;i<size;i++)
    {
        for(int j=i;j<size;j++)
        {
            if(arr[i]<arr[j])
            {
            int temp=arr[i];
            arr[i]=arr[j];
            arr[j]=temp;
            }
        }
    }
       for(int i=0;i<size;i+=2)
            printf("%d ",arr[i]);
       for(int i=size-1;i>0;i-=2)
            printf("%d ",arr[i]);
    return 0;
}

















